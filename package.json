{
    "name": "croissant",
    "version": "1.0.0",
    "description": "Croissant is the main Discord Bot for La Boulangerie Networks' Discord",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "start:dev": "ts-node-dev src/index.ts",
        "start:prod": "node dist/index.js",
        "build": "tsc",
        "watch": "tsc -w",
        "openapi": "npx openapi-typescript https://api.laboulangerie.net/openapi -o src/types/api.ts",
        "test:ts": "tsc --noEmit"
    },
    "author": "PainOchoco",
    "license": "MIT",
    "dependencies": {
        "@googleapis/sheets": "^4.0.1",
        "@wakeful-cloud/html-translator": "1.1.2",
        "@keyv/redis": "^4.3.4",
        "axios": "1.8.4",
        "discord.js": "14.18.0",
        "dotenv": "16.5.0",
        "googleapis": "148.0.0",
        "keyv": "^5.3.3",
        "luxon": "3.6.1",
        "openapi-fetch": "^0.13.5",
        "path": "0.12.7",
        "wikijs": "6.4.1"
    },
    "devDependencies": {
        "@types/luxon": "3.6.2",
        "@types/node": "^22.14.1",
        "openapi-typescript": "^7.6.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
