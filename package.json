{
    "name": "croissant",
    "version": "1.0.0",
    "description": "Croissant is the main Discord Bot for La Boulangerie Networks' Discord",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "start:dev": "ts-node-dev src/index.ts",
        "start:prod": "node dist/index.js",
        "build": "tsc",
        "watch": "tsc -w",
        "openapi": "npx openapi-typescript https://api.laboulangerie.net/openapi -o src/types/api.ts"
    },
    "author": "PainOchoco",
    "license": "MIT",
    "dependencies": {
        "@googleapis/sheets": "^4.0.1",
        "@keyv/redis": "^2.5.5",
        "@qdrant/openapi-typescript-fetch": "^1.2.1",
        "@wakeful-cloud/html-translator": "1.1.2",
        "discord.js": "^14.13.0",
        "dotenv": "16.0.3",
        "googleapis": "^111.0.0",
        "keyv": "^4.5.2",
        "luxon": "^3.2.1",
        "path": "0.12.7",
        "wikijs": "6.4.0"
    },
    "devDependencies": {
        "@types/luxon": "3.2.0",
        "@types/node": "^20.8.0",
        "openapi-typescript": "^6.7.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
